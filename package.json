{
  "name": "react-band",
  "version": "0.1.0",
  "description": "A simple react scaffold with webpack, babel, jest, enzyme and puppeteer.",
  "main": "src/index.jsx",
  "scripts": {
    "start": "better-npm-run start",
    "dev": "better-npm-run start",
    "lint": "eslint ./src/**/*.{js,jsx}",
    "lint:debug": "eslint --debug ./src/**/*.jsx",
    "lint:write": "eslint --debug ./src --fix",
    "test": "better-npm-run test",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand",
    "test:watch": "better-npm-run test:watch",
    "test:coverage": "better-npm-run test:coverage",
    "compile": "better-npm-run compile"
  },
  "betterScripts": {
    "start": {
      "command": "webpack-dev-server --config webpack.dev.config.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "test": {
      "command": "jest",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "test:watch": {
      "command": "jest --watch",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "test:coverage": {
      "command": "jest --coverage --colors",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "compile": {
      "command": "webpack --config webpack.prod.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hcl1687/react-band.git"
  },
  "keywords": [
    "react",
    "jest",
    "puppeteer",
    "eslint",
    "react-router",
    "babel"
  ],
  "author": "Chunlin He",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hcl1687/react-band/issues"
  },
  "homepage": "https://github.com/hcl1687/react-band#readme",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "better-npm-run": "^0.1.1",
    "css-loader": "^3.1.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^6.1.0",
    "eslint-config-standard": "^13.0.1",
    "eslint-config-standard-react": "^8.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-control-statements": "^2.2.1",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-standard": "^4.0.0",
    "extract-text-webpack-plugin": "^4.0.0-alpha.0",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.8.0",
    "jest-puppeteer": "^4.3.0",
    "jsdom": "^15.1.1",
    "postcss": "^7.0.17",
    "postcss-browser-reporter": "^0.6.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-nesting": "^7.0.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-reporter": "^6.0.1",
    "postcss-url": "^8.0.0",
    "prop-types": "^15.7.2",
    "puppeteer": "^1.19.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^2.3.5",
    "webpack": "^4.37.0",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "antd": "^3.26.12",
    "axios": "^0.19.0",
    "classnames": "^2.2.6",
    "intl-messageformat": "^6.0.1",
    "lodash": "^4.17.15",
    "promiseflow": "^1.0.10",
    "prop-types": "^15.7.2",
    "query-string": "^6.10.1",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "redux-logger": "^3.0.6",
    "redux-promise": "^0.6.0",
    "uuid": "^3.3.3"
  }
}
